<!-- This module contains the definitions for the CALS Table Model
     converted to XML.
-->

<!-- These definitions are not directly related to the table model, but are
     used in the default CALS table model and are usually defined elsewhere
     (and prior to the inclusion of this table module) in a CALS DTD. -->

<!ENTITY % bodyatt "">
<!ENTITY % secur "">

<!-- no if zero(s),
                                yes if any other digits value -->

<!ENTITY % yesorno 'CDATA'>
<!ENTITY % titles  ''>

<!-- default for use in entry content -->

<!ENTITY % paracon '#PCDATA'>

<!--
The parameter entities as defined below provide the CALS table model
as published (as part of the Example DTD) in MIL-HDBK-28001.

These following declarations provide the CALS-compliant default definitions
for these entities.  However, these entities can and should be redefined
(by giving the appropriate parameter entity declaration(s) prior to the
reference to this Table Model declaration set entity) to fit the needs
of the current application.
-->

<!ENTITY % tbl.table.name       "table">
<!ENTITY % tbl.table-titles.mdl "">
<!ENTITY % tbl.table-main.mdl   "(tgroup+)">
<!ENTITY % tbl.table.mdl        "%tbl.table-titles.mdl; %tbl.table-main.mdl;">
<!ENTITY % tbl.table.att        '
    tabstyle    CDATA           #IMPLIED
    tocentry    %yesorno;       #IMPLIED
    shortentry  %yesorno;       #IMPLIED
    orient      (port|land)     #IMPLIED
    pgwide      %yesorno;       #IMPLIED '>
<!ENTITY % tbl.tgroup.mdl       "colspec*,spanspec*,thead?,tfoot?,tbody">
<!ENTITY % tbl.tgroup.att       '
    tgroupstyle CDATA           #IMPLIED '>
<!ENTITY % tbl.hdft.mdl         "colspec*,row+">
<!ENTITY % tbl.row.mdl          "(entry|entrytbl)+">
<!ENTITY % tbl.entrytbl.mdl     "colspec*,spanspec*,thead?,tbody">
<!ENTITY % tbl.entry.mdl        "%paracon;|al|al-f|%txtrenv;|arg|d1entry|d2entry|imag">


<!-- =====  Element and attribute declarations follow. =====  -->

<!-- Table -->
<!ELEMENT %tbl.table.name; (%tbl.table.mdl;)>

<!--
	@attr frame Cadre autour de la table
	@attr colsep Filet de separation entre les colonnes
	@attr rowsep Filet de separation entre les lignes
	@attr tabstyle Style de table pour le maquetage
	@attr tocentry (a faire)
	@attr shortentry (a faire)
	@attr orient Orientation de la table par rapport a la page
	@attr pgwide Table en pleine page ou non
-->
<!ATTLIST %tbl.table.name;
        frame           (top|bottom|topbot|all|sides|none)      #IMPLIED
        colsep          %yesorno;                               #IMPLIED
        rowsep          %yesorno;                               #IMPLIED
        %tbl.table.att;
        %bodyatt;
        %secur;
>

<!-- Definition des formats de colonnes, header... -->
<!ELEMENT tgroup (%tbl.tgroup.mdl;) >

<!--
	@attr cols Nombre de colonnes de la table
	@attr tgroupstyle (a faire)
	@attr colsep Separateur de colonnes
	@attr rowsep Separateur de lignes
	@attr align Alignement du texte dans la table
	@attr char Caractere qui sera utilisé pour identifier le début du retrait si align = char
	@attr charoff Pourcentage de la largeur de la colonne fixant le decalage du retrait à partir de la gauche, si align = char
	@attr trgrsty style du tgroup
-->
<!ATTLIST tgroup
        cols            CDATA                                   #REQUIRED
        %tbl.tgroup.att;
        colsep          %yesorno;                               #IMPLIED
        rowsep          %yesorno;                               #IMPLIED
        align           (left|right|center|justify|char)        #IMPLIED
        char            CDATA                                   #IMPLIED
        charoff         CDATA                                   #IMPLIED
        %secur;
        tgrpsty         NMTOKEN         #IMPLIED
>

<!-- Secifications d'une colonne -->
<!ELEMENT colspec EMPTY >

<!--
	@attr colnum Numero de la colonne
	@attr colname Nom de la colonne
	@attr colwidth Largeur de la colonne
	@attr colsep Separateur de colonnes
	@attr rowsep Separateur de lignes
	@attr align Alignement du texte dans la table
	@attr char Caractere qui sera utilisé pour identifier le début du retrait si align = char
	@attr charoff Pourcentage de la largeur de la colonne fixant le decalage du retrait à partir de la gauche, si align = char
-->
<!ATTLIST colspec
        colnum          CDATA                                   #IMPLIED
        colname         CDATA                                   #IMPLIED
        colwidth        CDATA                                   #IMPLIED
        colsep          %yesorno;                               #IMPLIED
        rowsep          %yesorno;                               #IMPLIED
        align           (left|right|center|justify|char)        #IMPLIED
        char            CDATA                                   #IMPLIED
        charoff         CDATA                                   #IMPLIED
>

<!-- Specification des fusions de cellules -->
<!ELEMENT spanspec EMPTY >

<!--
	@attr namest Nom de la premiere colonne fusionee
	@attr nameend Nom de la derniere colonne fusionee
	@attr spanname Nom de la fusion
	@attr colsep Separateur de colonnes
	@attr rowsep Separateur de lignes
	@attr align Alignement du texte dans la table
	@attr char Caractere qui sera utilisé pour identifier le début du retrait si align = char
	@attr charoff Pourcentage de la largeur de la colonne fixant le decalage du retrait à partir de la gauche, si align = char
-->
<!ATTLIST spanspec
        namest          CDATA                                   #REQUIRED
        nameend         CDATA                                   #REQUIRED
        spanname        CDATA                                   #REQUIRED
        colsep          %yesorno;                               #IMPLIED
        rowsep          %yesorno;                               #IMPLIED
        align           (left|right|center|justify|char)        #IMPLIED
        char            CDATA                                   #IMPLIED
        charoff         CDATA                                   #IMPLIED
>

<!-- Header de la table. Repris en haut de chaque nouvelle page -->
<!ELEMENT thead (%tbl.hdft.mdl;)>

<!--
	@attr valign Alignement vertical du texte dans la table
-->
<!ATTLIST thead
        valign          (top|middle|bottom)                     #IMPLIED
        %secur;
>

<!-- Pas de page de la table. Repris en bas de chaque nouvelle page --> 
<!ELEMENT tfoot (%tbl.hdft.mdl;)>

<!--
	@attr valign Alignement vertical du texte dans la table
-->
<!ATTLIST tfoot
        valign          (top|middle|bottom)                     #IMPLIED
        %secur;
>

<!-- Corps de la table -->
<!ELEMENT tbody (row+)>

<!--
	@attr valign Alignement vertical du texte dans la table
-->
<!ATTLIST tbody
        valign          (top|middle|bottom)                     #IMPLIED
        %secur;
>

<!-- Ligne de la table -->
<!ELEMENT row (%tbl.row.mdl;)>

<!--
	@attr rowsep Separateur de lignes
	@attr valign Alignement vertical du texte dans la table
-->
<!ATTLIST row
        rowsep          %yesorno;                               #IMPLIED
        valign          (top|middle|bottom)                     #IMPLIED
        %secur;
>

<!-- (a faire) -->
<!ELEMENT entrytbl (%tbl.entrytbl.mdl;)>

<!--
	@attr cols Nombre de colonne
	@attr tgroupstyle (a faire)
	@attr colname Nom de la colonne
	@attr spanname Nom de la fusion
	@attr namest Nom de la premiere colonne fusionee
	@attr nameend Nom de la derniere colonne fusionee
	@attr colsep Separateur de colonnes
	@attr rowsep Separateur de lignes
	@attr align Alignement du texte dans la table
	@attr char Caractere qui sera utilisé pour identifier le début du retrait si align = char
	@attr charoff Pourcentage de la largeur de la colonne fixant le decalage du retrait à partir de la gauche, si align = char
-->
<!ATTLIST entrytbl
        cols            CDATA                                   #REQUIRED
        %tbl.tgroup.att;
        colname         CDATA                                   #IMPLIED
        spanname        CDATA                                   #IMPLIED
        namest          CDATA                                   #IMPLIED
        nameend         CDATA                                   #IMPLIED
        colsep          %yesorno;                               #IMPLIED
        rowsep          %yesorno;                               #IMPLIED
        align           (left|right|center|justify|char)        #IMPLIED
        char            CDATA                                   #IMPLIED
        charoff         CDATA                                   #IMPLIED
        %secur;
>

<!-- Cellule de la table -->
<!ELEMENT entry (%tbl.entry.mdl;)*>

<!--
	@attr colname Nom de la colonne
	@attr namest Nom de la premiere colonne fusionee
	@attr nameend Nom de la derniere colonne fusionee
	@attr spanname Nom de la fusion
	@attr morerows (a faire)
	@attr colsep Separateur de colonnes
	@attr rowsep Separateur de lignes
	@attr align Alignement du texte dans la table
	@attr char Caractere qui sera utilisé pour identifier le début du retrait si align = char
	@attr charoff Pourcentage de la largeur de la colonne fixant le decalage du retrait à partir de la gauche, si align = char
	@attr rotate Rotation sur le texte dans la cellule
	@attr valign Alignement vertical du texte dans la cellule
-->

<!ATTLIST entry
        colname         CDATA                                   #IMPLIED
        namest          CDATA                                   #IMPLIED
        nameend         CDATA                                   #IMPLIED
        spanname        CDATA                                   #IMPLIED
        morerows        CDATA                                   #IMPLIED
        colsep          %yesorno;                               #IMPLIED
        rowsep          %yesorno;                               #IMPLIED
        align           (left|right|center|justify|char)        #IMPLIED
        char            CDATA                                   #IMPLIED
        charoff         CDATA                                   #IMPLIED
        rotate          %yesorno;                               #IMPLIED
        valign          (top|middle|bottom)                     #IMPLIED
        %secur;
        ligdc           (oui|non)                               'non'
        mesure           CDATA          						#IMPLIED  
>

<!-- End of DocBook XML CALS Table Model V4.0 ............................. -->

