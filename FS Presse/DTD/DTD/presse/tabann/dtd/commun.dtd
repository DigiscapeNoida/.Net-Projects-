<!-- Ensemble d'annexes -->
<!ELEMENT annexes (annexe | formule)+>

<!-- Annexe -->
<!ELEMENT annexe (tit?, (p | intertit | tab | imag | refsource)+)>


<!-- ==================================================
     Les meta donnees d'un article
================================================== -->

<!-- Meta donnees d'un article -->
<!ELEMENT metaart (publ+, combid?, fichierlie*, matiere*)>

<!-- Meta donnees de publication -->
<!ELEMENT publ  (nomrev, daterev, revnumr, revnumrsup?, numeltr, partrev, pagerev, (partie, (d1par, (d2par, d3par?)?)?), dapar?, obs?) >

<!-- Reprise du titre de partie dans le chemin de fer -->
