	typemime CDATA #REQUIRED
	smi CDATA #IMPLIED
>

<!-- <b>Définition</b> : matiere pour les pack -->
<!ELEMENT matiere EMPTY >
<!--
@attr code : libelle de la matiere suivi de son code entité
-->
<!ATTLIST matiere
	code (affaires_thm125748 | civil_thm136153 | comptabilite_thm799265 | douanes_thm799266 | droit-international_thm381441
		| europe_thm751300 | fiscal_thm378261 | penal_thm382830 | procedure_thm139313 | profession_thm738806
		| public_thm482041 | social_thm353322) #REQUIRED
>

