      12/04/2006 VDZ Autorisé imag dans entry
-->

<!-- Tableau -->
<!ELEMENT tab   ((titab, sti1tab?, sti2tab?)? , legtab?, table*)>

<!ATTLIST tab
	position	(haut | bas | marge | interne)	"interne"
	rotation	(non | oui)			"non"
	>


<!-- Titre de tableau -->
<!ELEMENT titab (al)>

<!-- Sous titre de tableau de niveau 1 -->
<!ELEMENT sti1tab (al)>

<!-- Sous titre de tableau de niveau 2 -->
<!ELEMENT sti2tab (al)>

<!-- Legende de tableau -->
<!ELEMENT legtab (al)>


<!-- ==================================================
     Elements de la revue DO
================================================== -->

<!-- début de filet vertical à gauche -->
<!ELEMENT gfilet EMPTY >
<!-- fin de filet vertical à gauche -->
<!ELEMENT gxfilet EMPTY >

<!-- texte complémentaire -->
<!ELEMENT arg (al|tab|imag|gfilet|gxfilet)+>
<!-- @attr typearg  type de texte complémentaire
     @attr numarg   "oui" si le texte est numéroté
     -->
<!ATTLIST arg 
	typearg		(remarque | exemple | caspart | notabene | observations)	#IMPLIED
	numarg		(oui| non)	"non" >


<!-- division de niveau 1 dans une cellule de tableau -->
<!ELEMENT d1entry (tit?,(al|al-f|%txtrenv;|arg)*,d2entry*)>
<!-- division de niveau 2 dans une cellule de tableau -->
<!ELEMENT d2entry (tit?,(al|al-f|%txtrenv;|arg)*)>


<!-- Alinea de formule -->
<!ELEMENT al-f  (#PCDATA | emph2 | emph3 | ref-in | l | apnd | eqn | rtc|rtnc|rdec|rbiblio|rquid|rjc|rintra|rpubl|rjo|rjd|rccons |
               indic-in | variante | ajout-in | zoneczi | ligpoint)* >
<!--
	@attr type Attribut de mise en page de l'alinea de formule
-->
<!ATTLIST al-f
               type (normal | indent | retrait | centre | droit)  "normal"
                >

<!-- Formule telle que dans les collections JC -->
<!ELEMENT formule  (tiff,somform?,observff*,((((tbase | lchoix | indic-bl |
               artff),(tbase | lchoix | indic-bl | ajout-bl | artff)*),(divform1,(
               divform1 | ajout-bl | lchoix)*)?) | (divform1,(divform1 | ajout-bl |
               lchoix)*))?,an-form*) >
<!--
	@attr type Differentiation des formules. Correspont en general a un maquetage different
	@attr id Identifiant de la formule
	@attr idbdlc Identifiant dans une base de donnees (inutilise)
-->
<!ATTLIST formule
               type (FORMULE | CAS | DISCOURS | MODELE)  "FORMULE"
               id  ID    #IMPLIED
               idbdlc  CDATA    #IMPLIED  >

<!-- Titre de formule -->
<!ELEMENT tiff  (al-f) >

<!-- Sommaire de formule -->
<!ELEMENT somform    EMPTY  >

<!-- Observation en debut de formule -->
<!ELEMENT observff  (tiff?,(al | intertit | tab | cita-bl | imag)+) >

<!-- Texte de base de la formule -->
<!ELEMENT tbase  (ligpoint | al-f | intertiff | sep | tab | imag |
               cita-bl)+ >

<!-- Titre d'article de formule -->
<!ELEMENT tiartf  (desartf,num-art1,libartf?) >

<!-- Designation d'article de formule -->
<!ELEMENT desartf  (#PCDATA) >

<!-- Numero d'article de formule -->
<!ELEMENT num-art1  (#PCDATA) >

<!-- Libelle d'article de formule -->
<!ELEMENT libartf  (#PCDATA | emph3 | ref-in | l | apnd | eqn | rtc|rtnc|rdec|rbiblio|rquid|rjc|rintra|rpubl|rjo|rjd|rccons |
               indic-in | variante | ajout-in | zoneczi | ligpoint)* >

<!-- Division de niv1 d'article de formule -->
<!ELEMENT niv1artff  (tiff,((((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*),(niv2artff,(niv2artff |
               ajout-bl | lchoix)*)?) | (niv2artff,(niv2artff | ajout-bl | lchoix)*))?) >

<!-- Division de niv2 d'article de formule -->
<!ELEMENT niv2artff  (tiff,((((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*),(niv3artff,(niv3artff |
               ajout-bl | lchoix)*)?) | (niv3artff,(niv3artff | ajout-bl | lchoix)*))?) >

<!-- Division de niv3 d'article de formule -->
<!ELEMENT niv3artff  (tiff,((((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*),(niv4artff,(niv4artff |
               ajout-bl | lchoix)*)?) | (niv4artff,(niv4artff | ajout-bl | lchoix)*))?) >

<!-- Division de niv4 d'article de formule -->
<!ELEMENT niv4artff  (tiff,((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*)?) >

<!-- Bloc d'ajout -->
<!ELEMENT ajout-bl  (tiff,(((ligpoint | al-f | intertiff | sep |
               tab | imag | cita-bl) | divform1 | divform2 | divform3 | divform4 |
               artff | niv1artff | niv2artff | niv3artff | niv4artff) | indic-bl |
               lchoix)+,pours?)>

<!-- Annexe de formule -->
<!ELEMENT an-form  (tiff,((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*)) >
<!-- @attr id Identifiant de l'annexe de formule -->
<!ATTLIST an-form
               id  ID    #IMPLIED  >

<!-- Indication inline -->
<!ELEMENT indic-in  (#PCDATA | rtc|rtnc|rdec|rbiblio|rquid|rjc|rintra|rpubl|rjo|rjd|rccons | apnd)* >

<!-- Variante (ou : ..) -->
<!ELEMENT variante  (indic-in,trempl)+ >

<!-- Ajout inline -->
<!ELEMENT ajout-in  (indic-in,trempl)+ >

<!-- Texte de remplacement -->
<!ELEMENT trempl  (#PCDATA | rtc|rtnc|rdec|rbiblio|rquid|rjc|rintra|rpubl|rjo|rjd|rccons | zoneczi | apnd)* >

<!-- Intetitre de formule -->
<!ELEMENT intertiff  (al-f) >

<!-- Separateur dans une formule -->
<!ELEMENT sep   EMPTY  >
<!-- @attr position Position du filet de separation sur la largeur de la page -->
<!ATTLIST sep
               position (center | left | right)  "center" >

<!-- Item de choix -->
<!ELEMENT choix  (tiff?,(((ligpoint | al-f | intertiff | sep |
               tab | imag | cita-bl) | divform1 | divform2 | divform3 | divform4 |
               artff | niv1artff | niv2artff | niv3artff | niv4artff) | indic-bl |
               ajout-bl)+) >

<!-- Poursuivre ensuite -->
<!ELEMENT pours   EMPTY  >

<!-- Division de niv1 de formule -->
<!ELEMENT divform1  (tiff,((((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*),(divform2,(divform2 |
               ajout-bl | lchoix)*)?) | (divform2,(divform2 | ajout-bl | lchoix)*))) >
<!-- @attr id Identifiant de la division de formule -->
<!ATTLIST divform1
               id  ID    #IMPLIED  >

<!-- Division de niv2 de formule -->
<!ELEMENT divform2  (tiff,((((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*),(divform3,(divform3 |
               ajout-bl | lchoix)*)?) | (divform3,(divform3 | ajout-bl | lchoix)*))) >
<!-- @attr id Identifiant de la division de formule -->
<!ATTLIST divform2
               id  ID    #IMPLIED  >

<!-- Division de niv3 de formule -->
<!ELEMENT divform3  (tiff,((((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*),(divform4,(divform4 |
               ajout-bl | lchoix)*)?) | (divform4,(divform4 | ajout-bl | lchoix)*))) >
<!-- @attr id Identifiant de la division de formule -->
<!ATTLIST divform3
               id  ID    #IMPLIED  >

<!-- Division de niv4 de formule -->
<!ELEMENT divform4  (tiff,((tbase | lchoix | indic-bl | artff),(tbase |
               lchoix | indic-bl | ajout-bl | artff)*)) >
<!-- @attr id Identifiant de la division de formule -->
<!ATTLIST divform4
               id  ID    #IMPLIED  >

<!-- Zone de saisie -->
